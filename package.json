{
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --hot --config ./webpack.dev.js",
    "build": "NODE_ENV=production webpack --progress --config ./webpack.prod.js",
    "lint": "yarn lint-js && yarn lint-spec",
    "lint-js": "eslint --ext .jsx --ext .js --ignore-pattern \"*.spec.*\" --ignore-pattern '*/test/*' examples core app",
    "lint-js-fix": "eslint --fix --ext .jsx --ext .js --ignore-pattern \"*.spec.*\" examples core app",
    "lint-spec": "eslint --ext .spec.jsx --ext .spec.js -c .eslintrc_spec.json examples core app",
    "lint-spec-fix": "eslint --fix --ext .spec.jsx --ext .spec.js -c .eslintrc_spec.json examples core app",
    "test": "yarn lint-js && yarn lint-spec && jest",
    "test-watch": "jest --watch",
    "test-ci": "jest --ci --coverage",
    "styleguide": "styleguidist server",
    "styleguide-build": "styleguidist build",
    "prettier": "prettier --write \"{app,core,examples}/**/{*.jsx,*.js}\""
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.1.2",
    "axios": "^0.17.1",
    "firebase": "^4.6.2",
    "i18n-js": "^3.0.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "ramda": "^0.25.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-portal": "^4.1.1",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-css-modules": "^3.3.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "css-loader": "^0.28.7",
    "enzyme": "^2.7.1",
    "eslint": "^4.7.0",
    "eslint-config-originate": "^2.0.3",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
    "jest": "^21.2.1",
    "prettier": "^1.7.0",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-docgen-annotation-resolver": "^1.0.0",
    "react-styleguidist": "^6.1.0",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.6.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.3.0",
    "source-map-support": "^0.4.18",
    "style-loader": "^0.18.2",
    "svg-loader": "^0.0.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|png|ico|ttf|eot)$": "<rootDir>/test/fileMock.js",
      "\\.(svg)$": "<rootDir>/test/svgMock.js",
      "\\.(css)$": "<rootDir>/test/styleMock.js",
      "^core(.*)$": "<rootDir>/core/$1",
      "^app(.*)$": "<rootDir>/app/$1",
      "^todo(.*)$": "<rootDir>/examples/todo/$1",
      "^firebase_authentication(.*)$": "<rootDir>/examples/firebase_authentication/$1"
    }
  }
}