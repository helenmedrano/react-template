{
  "extends": "react-app",
  "plugins": ["prettier"],
  "globals": {
    "logger": true,
    "expect": true,
    "sinon": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-expressions": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.dev.js"
      }
    }
  }
}
