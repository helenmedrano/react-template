{
  "parser": "babel-eslint",
  "extends": "originate/react",
  "globals": {
    "logger": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.dev.js"
      }
    }
  },
  "overrides": {
    "files": [ "**/*.spec.js", "**/test/*.js" ],
    "globals": {
      "logger": true,
      "expect": true,
      "sinon": true,
      "jest": true
    }
  }
}
